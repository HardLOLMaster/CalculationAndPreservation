<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file></welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Contract list</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.ContractList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Contract</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.ContractServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateServlet</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.CreateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PolicyHolderServlet</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.PolicyHolderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewPh</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.NewPh</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePhServlet</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.ChangePhServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveContract</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.SaveContract</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateServlet</servlet-name>
        <servlet-class>com.mycompany.calculationandpreservation.servlets.UpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Contract list</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Contract</servlet-name>
        <url-pattern>/openContract</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateServlet</servlet-name>
        <url-pattern>/createContract</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PolicyHolderServlet</servlet-name>
        <url-pattern>/phForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewPh</servlet-name>
        <url-pattern>/newPh</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePhServlet</servlet-name>
        <url-pattern>/changePh</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveContract</servlet-name>
        <url-pattern>/saveContract</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateServlet</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
